<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.predev.gymcrm.repository.CommonMapper">
    <resultMap id="TimeResultMap" type="com.predev.gymcrm.entity.Time">
        <id property="timeId" column="time_id"/>
        <result property="timeDuration" column="time_duration"/>
        <result property="createDate" column="create_date"/>
        <result property="updateDate" column="update_date"/>
    </resultMap>

    <select id="getTimes" resultMap="TimeResultMap">
        select
            time_id,
            time_duration,
            create_date,
            update_date
        from
            time_tb
    </select>
</mapper>